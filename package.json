{
  "name": "@buxlabs/ast",
  "version": "0.8.0",
  "description": "abstract syntax tree class for js",
  "main": "index.js",
  "scripts": {
    "test": "ava \"test/spec/basic.js\"",
    "coverage": "nyc --check-coverage --statements 100 --lines 100 --functions 100 --branches 100 npm test",
    "lint": "eslint index.js test/**/*.js",
    "benchmark": "ava \"test/spec/benchmark.js\"",
    "build": "browserify index.js -s=AbstractSyntaxTree | uglifyjs > build/ast.js"
  },
  "engines": {
    "node": ">=6.11.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buxlabs/ast.git"
  },
  "keywords": [
    "ast",
    "javascript",
    "abstract syntax tree"
  ],
  "author": "Emil Ajdyna <emil@ajdyna.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buxlabs/ast/issues"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/buxlabs/ast#readme",
  "devDependencies": {
    "ava": "^0.23.0",
    "benchmark": "^2.1.4",
    "browserify": "^14.5.0",
    "eslint": "^4.10.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "nyc": "^11.3.0",
    "uglify-es": "^3.1.7"
  },
  "dependencies": {
    "comparify": "^0.2.0",
    "escodegen": "^1.9.0",
    "espree": "^3.5.1",
    "esquery": "^1.0.0",
    "estemplate": "^0.5.1",
    "estraverse": "^4.2.0",
    "prettier": "^1.7.4",
    "to-ast": "^1.0.0"
  }
